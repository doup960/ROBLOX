local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Doups CISH", HidePremium = false, SaveConfig = false, ConfigFolder = "DSH"})

local function notifyWIP()
    OrionLib:MakeNotification({
        Name = "Work In Progress!",
        Content = "Im currently working on adding this feature to the game! If you have what I need to make this please ping me in my discord server! (discord.gg/spJewPjrUZ)",
        Image = "rbxassetid://4483345998",
        Time = 5
    })
end

local circlespeed = 0.1
local clickspeed = 0.5
local spherespeed = 0.1


local CircleInc = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = CircleInc:AddSection({
	Name = "Automation"
})

local player = game.Players.LocalPlayer
local isToggled = false

CircleInc:AddToggle({
	Name = "Auto Circles",
	Default = false,
	Callback = function(Value)
		isToggled = Value
		while isToggled do
            wait(circlespeed)
            local stuff = game:GetService("Workspace").Stuff:GetChildren()
            
            if #stuff > 0 and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                local randomPart = stuff[math.random(1, #stuff)]
                player.Character.HumanoidRootPart.CFrame = randomPart.CFrame
            end
        end
	end    
})

CircleInc:AddSlider({
	Name = "Auto Circle Speed",
	Min = 0.05,
	Max = 1,
	Default = 0.15,
	Color = Color3.fromRGB(255,255,255),
	Increment = 0.05,
	ValueName = "Seconds",
	Callback = function(Value)
		circlespeed = Value
	end    
})

local isToggled2 = false
CircleInc:AddToggle({
	Name = "Auto Clicks",
	Default = false,
	Callback = function(Value)
		isToggled2 = Value
		while isToggled2 do
         wait(clickspeed)
         player.Character.HumanoidRootPart.CFrame = CFrame.new(105, 4, -17)
         end
    end
})

CircleInc:AddSlider({
	Name = "Auto Click Speed",
	Min = 0.1,
	Max = 2.5,
	Default = 0.5,
	Color = Color3.fromRGB(255,255,255),
	Increment = 0.1,
	ValueName = "Seconds",
	Callback = function(Value)
		clickspeed = Value
	end    
})


local isToggled3 = false
CircleInc:AddToggle({
	Name = "Auto Spheres",
	Default = false,
	Callback = function(Value)
		isToggled3 = Value
		while isToggled3 do
            wait(spherespeed)
            local spheres = game:GetService("Workspace").Spheres:GetChildren()
            
            if #spheres > 0 and player.Character and player.Character:FindFirstChild("HumanoidRootPart") then
                local randomPart = spheres[math.random(1, #spheres)]
                player.Character.HumanoidRootPart.CFrame = randomPart.CFrame
            end
        end
	end    
})

CircleInc:AddSlider({
	Name = "Auto Sphere Speed",
	Min = 0.05,
	Max = 1,
	Default = 0.15,
	Color = Color3.fromRGB(255,255,255),
	Increment = 0.05,
	ValueName = "Seconds",
	Callback = function(Value)
		spherespeed = Value
	end    
})

local Section = CircleInc:AddSection({
	Name = "Teleports"
})


local Other = Window:MakeTab({
	Name = "Misc.",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

local Section = Other:AddSection({
	Name = "Teleports"
})

Other:AddButton({
	Name = "Circles",
	Callback = function()
      		player.Character.HumanoidRootPart.CFrame = CFrame.new(0, 3, 9)
  	end    
})

Other:AddButton({
	Name = "Clicks",
	Callback = function()
      		player.Character.HumanoidRootPart.CFrame = CFrame.new(105, 4, -17)
  	end    
})

Other:AddButton({
	Name = "Upgrade Emp.",
	Callback = function()
      		player.Character.HumanoidRootPart.CFrame = CFrame.new(29, 3, 97)
  	end    
})

Other:AddButton({
	Name = "Tires",
	Callback = function()
      		player.Character.HumanoidRootPart.CFrame = CFrame.new(-62, 3, 50)
  	end    
})

local Section = Other:AddSection({
	Name = "Work In Progress"
})

Other:AddButton({
	Name = "Cash",
	Callback = function()
      	notifyWIP()
  	end    
})

Other:AddButton({
	Name = "Supersede",
	Callback = function()
      	notifyWIP()
  	end    
})

Other:AddButton({
	Name = "Circle Fighting",
	Callback = function()
      	notifyWIP()
  	end    
})

OrionLib:Init()
