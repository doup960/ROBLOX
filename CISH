local Rayfield = loadstring(game:HttpGet('https://sirius.menu/rayfield'))()
print("Rayfield Injected")

local Window = Rayfield:CreateWindow({
    Name = "Circle Incremental Script Hub",
    LoadingTitle = ".gg/",
    LoadingSubtitle = "by doup96 (doup96 on discord)",
    ConfigurationSaving = {
       Enabled = true,
       FolderName = nil,
       FileName = "Circle Incremental Hub"
    },
    Discord = {
       Enabled = false,
       Invite = "noinvitelink",
       RememberJoins = true
    },
    KeySystem = false,
    KeySettings = {
       Title = "Untitled",
       Subtitle = "Key System",
       Note = "No method of obtaining the key is provided",
       FileName = "Key",
       SaveKey = true,
       GrabKeyFromSite = false,
       Key = {"Hello"}
    }
 })

 local CircleTab = Window:CreateTab("Circle Incremental", 4483362458)
 local Section = CircleTab:CreateSection("Circles")

 local Toggle = CircleTab:CreateToggle({
    Name = "Teleport To Circles",
    CurrentValue = false,
    Flag = "CircleCollectToggle",
    Callback = function(Value)
        while task.wait(0.01) do
            for i, v in pairs(game:GetService("Workspace").Stuff:GetChildren()) do
                 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
            end
        end
    end,
 })

 local Toggle = CircleTab:CreateToggle({
    Name = "Teleport To Spheres",
    CurrentValue = false,
    Flag = "SphereCollectToggle",
    Callback = function(Value)
        while task.wait(0.01) do
            for i, v in pairs(game:GetService("Workspace").Spheres:GetChildren()) do
                 game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = v.CFrame
            end
        end
    end,
 })
